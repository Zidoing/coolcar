<map id="map"
     longitude="{{location.longitude}}"
     latitude="{{location.latitude}}"
     scale="{{scale}}"
     setting="{{setting}}"
     enable-overlooking="{{isOverLooking}}"
     enable-3D="{{is3D}}"
     markers="{{markers}}"
>
    <cover-view class="icon-container">
        <cover-image src="/resources/account.png"></cover-image>
        <cover-image src="/resources/location.png"
                     bindtap="onMyLocationTap"
        ></cover-image>
    </cover-view>
    <cover-view class="btn-container bg-grey" bindtap="onScanClick">
        <cover-view>扫码租车</cover-view>
    </cover-view>
</map>